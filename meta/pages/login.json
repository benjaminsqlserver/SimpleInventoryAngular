{
  "access": [
    "Everybody"
  ],
  "components": [
    {
      "components": [
        {
          "name": "pageTitle",
          "size": "H1",
          "text": "Login",
          "type": "heading"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "allowRegister": false,
                  "allowResetPassword": false,
                  "events": {
                    "login": [
                      {
                        "error": [
                          {
                            "detail": "${result.error.message}",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Security.login",
                        "parameters": [
                          {
                            "name": "username",
                            "value": "${event.username}"
                          },
                          {
                            "name": "password",
                            "value": "${event.password}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "register": [
                      {
                        "path": "register-application-user",
                        "type": "popup"
                      }
                    ]
                  },
                  "loginText": "Login",
                  "name": "login0",
                  "passwordRequired": "Password is required",
                  "passwordText": "Password",
                  "registerMessageText": "Don't have an account yet?",
                  "registerText": "Sign up",
                  "resetPasswordText": "Forgot password",
                  "type": "login",
                  "userRequired": "Username is required",
                  "userText": "Username"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": []
  },
  "layout": "Login",
  "name": "Login"
}